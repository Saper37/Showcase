<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_calorescene"
    android:name="calore_scene"
    android:tag = "calore_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".CaloreScene">

    <androidx.constraintlayout.widget.ConstraintLayout

        android:id="@+id/calorelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/calore_return_button"
            android:layout_width="75dp"
            android:layout_height="82dp"
            android:layout_marginTop="532dp"
            android:text="@string/calore_return"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">


        </Button>

        <TextView
            android:id="@+id/calore_text"
            android:layout_width="162dp"
            android:layout_height="40dp"
            android:text="@string/calore_view"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.07">

        </TextView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>